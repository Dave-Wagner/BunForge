{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js(doc)?|jsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "fallback-dark",
      "base_url": "https://dave-wagner.github.io/BunForge/",
      "github_url": "https://github.com/dave-wagner/BunForge",
      "github_repo": "dave-wagner/BunForge",
      "github_branch": "main",
      "homepageTitle": "BunForge API Docs",
      "includeFilesListInHomepage": true
    },
    "markdown": {
      "hardwrap": false,
      "idInHeadings": true
    },
    "prism-theme": "prism-custom",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": ["plugins/markdown", "plugins/summarize"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "metadata": {
    "title": "bunForge"
  }
}
